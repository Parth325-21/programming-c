#include<stodio.h>
#include<conio.h>
void main()
{
float n,A;
clrscr();
printf("enter the value of sales");
scanf("%f",& n);
if(n>50000)
{
A=n*0.1;
printf("the commission of the salesman %f",A);
}
else if (n>=50000 && n<=40000)
{
A=n*0.07;
printf("the commission of the salesman %f",A);
}
else if (n>=30000 && n<=40000)
{
A=n*0.05;
printf("the commission of the salesman %f",A);
}
else
{
A=n*0.02;
printf("the commision of the salesman %f",A);
getch();
}
}
