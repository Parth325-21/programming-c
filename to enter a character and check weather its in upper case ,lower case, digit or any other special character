#include<stodio.h>
#include<conio.h>
void main()
{
char X;
clrscr();
printf("enter the character");
scanf("%c",& X);
if(X>='A'&& X<='Z')
{
printf("character is na upper case");
}
else if(X>='a'&& X<='z')
{
printf("character is a lower case");
}
else if(X>=0&& X<=9)
{
printf("it is a digit");
}
else
{
printf("the character is a special character");
getch();
}
}
